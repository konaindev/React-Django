{
  "name": "remarkably-www",
  "version": "0.0.1",
  "private": true,
  "license": "copyright",
  "scripts": {
    "build": "webpack --progress --colors --hide-modules --config=webpack.dev.js",
    "build:prod": "webpack --progress --colors --hide-modules --config=webpack.prod.js",
    "heroku-postbuild": "webpack --progress --config=webpack.prod.js",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook -o .out",
    "deploy-storybook": "rm -rf .out && build-storybook -o .out && echo 'storybook.remarkably.io' > '.out/CNAME' && storybook-to-ghpages --ci --existing-output-dir=.out --host-token-env-variable=GITHUB_TOKEN",
    "test-ui": "jest --runInBand --silent --ci",
    "test-ui-update-snapshots": "jest --updateSnapshot",
    "dump-local-fixtures": "python manage.py dumpdata --natural-foreign --natural-primary projects email_app crm analytics auth users contenttypes geo.address > data/dumped/latest.json",
    "upload-s3-fixtures": "aws s3 cp data/dumped/latest.json s3://fa22980d-b1d2-453b-8133-307ad70ccc55/$(git log --pretty=format:'%h' -n 1).latest.json",
    "update-s3-fixtures": "aws s3 cp s3://fa22980d-b1d2-453b-8133-307ad70ccc55/$(git log --pretty=format:'%h' -n 1).latest.json s3://fa22980d-b1d2-453b-8133-307ad70ccc55/latest.json",
    "fetch-s3-fixtures": "aws s3 cp s3://fa22980d-b1d2-453b-8133-307ad70ccc55/$(git log --pretty=format:'%h' -n 1).latest.json data/dumped/latest.json",
    "fetch-latest-fixtures": "aws s3 cp s3://fa22980d-b1d2-453b-8133-307ad70ccc55/latest.json data/dumped/latest.json",
    "chromatic": "./node_modules/.bin/chromatic test",
    "deploy-frontend-staging": "aws s3 sync dist/ s3://app-staging.remarkably.io",
    "deploy-frontend-production": "aws s3 sync dist/ s3://app.remarkably.io",
    "run-build-ui": "yarn build && ./node_modules/.bin/webpack-dev-server --open --mode development --config webpack.dev.js --history-api-fallback",
    "invalidate-cdn-staging": "aws cloudfront create-invalidation --distribution-id ${CF_DIST_STAGING} --paths /",
    "invalidate-cdn-production": "aws cloudfront create-invalidation --distribution-id ${CF_DIST_PRODUCTION} --paths /"
  },
  "storybook-deployer": {
    "gitUsername": "Storybook Deployer",
    "gitEmail": "storybook.deployer@remarkably.io",
    "commitMessage": "Deploy Storybook to GitHub Pages [skip ci]"
  },
